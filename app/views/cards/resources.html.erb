<h5></h5>

<%= form_tag("resources2", :enctype =>"multipart/form-data") do %>
   
<% cname = Cycle.all %>
<% card_epic = Card.where("activity_type LIKE 'Strategic' ") %>

<div class="card">
  <div class="card-header">
    <h4> Update Resources for Strategic Projects</h4>
  </div>  
<div class="card-body">  

<div class="row ml-1">
    <div class="field">
     <%= label_tag :cycle %>
     <%= select_tag "cycle", options_from_collection_for_select(cname, "cycle_name", "cycle_name")  %>
  
   </div>
</div>

<div class="row ml-1">
    <div class="field">
     <%= label_tag :project %>
     <%= select_tag "epic", options_from_collection_for_select(card_epic, "short_name", "title")  %>
     
   </div>
</div>




</div>
   <div class="card-footer">
   
   <div class="actions">
      <%= submit_tag 'Next' %>
   </div>
 
  
</div>
</div>
<% end %>