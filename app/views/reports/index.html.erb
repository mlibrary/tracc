


<% cname = Cycle.all 

cname_array = ["All"]

cname.each do |cycle| 
 cname_array << cycle.cycle_name
end

status = Status.all
status_array = Array.new
status_array = ["All"]
status.each do |one|
  status_array << one.status
end   
%>

<%= form_tag("reports/export.csv", :enctype =>"multipart/form-data", :method => :get ) do %>

 <div class="card">
  <div class="card-header">
    <h5>Reports: Export</h5>
  </div>  
  <div class="card-body">
 <div class="form-group">
    <div  class="col-sm-2">Type:</div>
    <div class="col-sm-10">
      <%= select_tag "type", options_for_select(["Projects","Tracks","Comments","Staff Chips","Assignments","Objectives","Cycle Review"], "Projects")  %>
    </div>
  </div>


  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-primary">Export</button>
    </div>
  </div>
 </div>
</div>
<% end %>
