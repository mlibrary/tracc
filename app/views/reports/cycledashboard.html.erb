<h5></h5>

   
<% cname = Cycle.all %>

<div class="row" style="padding-top: 20px">
 <div class="col-md-6">
 <%= form_tag("strategic", :enctype =>"multipart/form-data") do %>
 <div class="card">
  <div class="card-header">
    <h4> LIT Internal Dashboard for Strategic Projects</h4>
  </div>  
 <div class="card-body">  

  <div class="row ml-1">
    <div class="field">
     <%= label_tag :cycle %>
     <%= select_tag "cycle", options_from_collection_for_select(cname, "cycle_name", "cycle_name")  %> 
   </div>
 </div>
 </div>
 <div class="card-footer"> 
   <div class="actions">
      <%= submit_tag 'Next >>' %>
   </div>
 </div>
</div>
 <% end %>
</div>

 <div class="col-md-6">
 <%= form_tag("dashboard", :enctype =>"multipart/form-data") do %>
 <div class="card">
  <div class="card-header">
    <h4> Public Dashboard for Strategic Projects</h4>
  </div>  
 <div class="card-body">  

  <div class="row ml-1">
    <div class="field">
     <%= label_tag :cycle %>
     <%= select_tag "cycle", options_from_collection_for_select(cname, "cycle_name", "cycle_name")  %> 
   </div>
 </div>
 </div>
 <div class="card-footer"> 
   <div class="actions">
      <%= submit_tag 'Next >>' %>
   </div>
 </div>
 <% end %>
</div>
</div>
