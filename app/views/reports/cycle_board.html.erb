  nameofmonths = ['','Jan', 'Feb','March','April','May','June','July','Aug', 'Sep','Oct','Nov','Dec']
  cur_cycle = Cycle.where("cycle_name LIKE '"+cycle+"'")
  s = cur_cycle.first.start.month
  stmonth = ""
  endmonth = ""
  months = ["","","","",""]
  
  i = 0 
  while (i<4) do
    months[i] = nameofmonths[s]
    i = i + 1
    if s<=12
      s = s +1 
    else
      s =1 
    end  
   
  end

